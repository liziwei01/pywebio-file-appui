#!/bin/bash
echo "alias ll='ls -al'" >> ~/.bashrc
source ~/.bashrc

mv /home/liziwei01/OpenSource/github.com/liziwei01/pywebio-file-appui/commit-msg /home/liziwei01/OpenSource/github.com/liziwei01/pywebio-file-appui/.git/hooks/
